class Solution {
    public int minSubArrayLen(int target, int[] nums) {
     int n=nums.length;
        int s=0,curr=0,res=Integer.MAX_VALUE;
        int diff;

        for (int e=0;e<n;e++){


            curr+=nums[e];

            while (curr>=target){

                diff=e-s+1;
                res=Math.min(res,diff);

                curr=curr-nums[s];
                s++;
            }
        }

        return res== Integer.MAX_VALUE ? 0 : res;   
    }
}
